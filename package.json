{
  "name": "frame",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "scripts": {
    "all:install": "npm-run-all --serial install:*",
    "all:start": "npm-run-all --parallel start:*",
    "install:main": "pnpm -F @frame/main install",
    "start:main": "node frame-cli/bin/frame-cli.js serve main",
    "build:main": "frame-cli build main",
    "start:timpage": "frame-cli serve timpage",
    "build:timpage": "frame-cli build timpage",
    "lint:eslint": "eslint --cache --max-warnings 0  \"packages/**/*.{vue,js,ts}\" --fix",
    "lint:prettier": "prettier --write  \"packages/**/*.{js,ts,json,css,scss,vue,html}\"",
    "lint:stylelint": "stylelint --cache --fix \"packages/**/*.{html,vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/.lintstagedrc.cjs",
    "lint:pretty": "pretty-quick --staged",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "author": "nemo",
  "license": "ISC",
